<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_map">

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.parking.ui.map.MapFragment"
        android:label="@string/title_map"
        tools:layout="@layout/activity_map" />

    <fragment
        android:id="@+id/navigation_personal"
        android:name="com.example.parking.ui.chat.ChatFragment"
        android:label="@string/title_personal"
        tools:layout="@layout/fragment_personal" />

    <fragment
        android:id="@+id/navigation_chat"
        
        android:name="com.example.parking.ui.personal.PersonalFragment"
        android:label="@string/title_board"
        tools:layout="@layout/fragment_chat" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.parking.ui.setting.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/activity_setting" />
</navigation>